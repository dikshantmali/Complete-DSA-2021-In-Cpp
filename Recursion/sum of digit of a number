
#include<bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;

int sum(int n)
{
  if (n == 0)
  {
    return 0;
  }
  int x = sum(n/10);
  return x + n%10;

}

int32_t main() {
