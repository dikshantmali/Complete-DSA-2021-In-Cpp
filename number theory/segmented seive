#include<bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;
bool isprime(long long int i)
{
    if(i==2)
    return true;
    if(i%2 == 0)
    return false;
    for(int j=3;j <= sqrt(i);j++)
    {
        if(i%j == 0)
        return false;
    }
    return true;
}
    vector<long long> findprime(long long int R)
    {
        vector<long long> a;
        for(int i=2;i<=sqrt(R);++i)
        {
            if(isprime(i))
            a.push_back(i);
        }
        return a;
    }
int32_t main() {
  #ifndef ONLINE_JUDGE
  freopen("input.txt","r",stdin);
  freopen("output.txt","w",stdout);
  #endif
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);

  long long int L,R;
  cin>>L>>R;
  vector<long long> prime = findprime(R);

        vector<long long int> isprime(R-L+1,1);
        
        for(long long int i=0;i<prime.size();i++)
        {
            long long int sm=(L/prime[i])*prime[i];
            if(sm < L)
            sm = sm + prime[i];

            for(long long int j=sm;j<=R;j = j+prime[i])
            {
              if (j != prime[i])
              {
                /* code */
                isprime[j-L] = 0;
              }
            }
                
            
            
            
        }


        long long int  ctr = 1;
        for(int i=L;i<=R;i++)
        {
            if(isprime[i-L] == 1)
            {
        ctr = ctr * i;
                
            }
        }

        cout<< ctr%mod;

  return 0;
}
