#include<bits/stdc++.h>
using namespace std;
#define ll long long


signed main() {
  #ifndef ONLINE_JUDGE
  freopen("input.txt","r",stdin);
  freopen("output.txt","w",stdout);
  #endif
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  int n;
  cin>>n;
  int pos;
  cin>>pos;


  n = (~(1 << pos) & n);
  cout<<n;
  


    return 0;
  
  
  
}
